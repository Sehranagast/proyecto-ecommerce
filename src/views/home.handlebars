<html>
  <head>
    <meta charset="utf-8" />
    <title>Product List</title>
  </head>
  <body>
    <ul>
      {{#each productsList}}
        <li>
          <h2>{{title}}</h2>
          <p>{{description}}</p>
          <p>Precio: {{price}}</p>
          <img src="{{thumbnail}}" alt="{{title}}" />
          <p>{{code}}</p>
          <p>{{stock}}</p>
        </li>
      {{/each}}
    </ul>
  </body>
</html>